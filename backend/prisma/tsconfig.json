{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": "..",
    "outDir": "../dist-seed",
    "paths": {
      "@/*": ["../src/*"],
      "@modules/*": ["../src/modules/*"],
      "@middleware/*": ["../src/middleware/*"],
      "@utils/*": ["../src/utils/*"],
      "@config/*": ["../src/config/*"],
      "@app-types/*": ["../src/types/*"],
      "@database/*": ["../src/database/*"]
    }
  },
  "include": ["seed.ts"]
}
